config_version=5

[application]

config/name="Damascus Proyecto Merged"
run/main_scene="res://core.tscn"
config/features=PackedStringArray("4.2", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

SettingsDataContainer="*res://autoloads/SettingsDataContainer.gd"
SettingsSignalBus="*res://autoloads/SettingsSignalBus.gd"
SaveManager="*res://autoloads/SaveManager.gd"
PlayerVariables="*res://autoloads/PlayerVariables.gd"
MultiplayerInput="*res://addons/multiplayer_input/multiplayer_input.gd"
PlayerManager="*res://autoloads/PlayerManager.gd"

[display]

window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Testeo Proyecto"

[editor_plugins]

enabled=PackedStringArray("res://addons/multiplayer_input/plugin.cfg")

[file_customization]

folder_colors={
"res://Assets/": "yellow",
"res://Characters/": "orange",
"res://Characters/Unused/": "gray",
"res://Level/": "red",
"res://Menu/": "green",
"res://MishoCosas/": "purple",
"res://Music/": "blue",
"res://addons/": "orange",
"res://autoloads/": "gray",
"res://resources/": "gray"
}

[filesystem]

import/fbx/enabled=false

